<h1>Listing Socials</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Address</th>
      <th>Postcode</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @socials.each do |social| %>
      <tr>
        <td><%= social.title %></td>
        <td><%= social.description %></td>
        <td><%= social.address %></td>
        <td><%= social.postcode %></td>
        <td><%= social.user_id %></td>
        <td><%= link_to 'Show', user_social_path(social.user, social) %></td>
        <td><%= link_to 'Edit', edit_user_social_path(social.user, social) %></td>
        <td><%= link_to 'Destroy', user_social_path(social.user, social), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Social', new_user_social_path(current_user) %>
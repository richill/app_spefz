<div class="user_dashboard_admin_inquiries">
  <!-- upper_data_content -->
  <div class="medium-12 columns upper_data_content" style="margin-bottom:0px;">
    <div class="medium-2 columns left_data responsive_data stats_tab_01">
      <div class="data_content">
        <div class="header"> total inquiries</div>
        <div class="header_data"><span class="header_icon"><i class="fa fa-rss" aria-hidden="true"></i></span><span><%= @contacts.inquiry_unresolved.count %></span></div>
      </div>
    </div>
    <div class="medium-2 columns middle_data responsive_data stats_tab_02">
      <div class="data_content">
        <div class="header"> general enquiry</div>
        <div class="header_data"><span class="header_icon"><i class="fa fa-rss" aria-hidden="true"></i></span>
          <span><%= @contacts.general_enquiry.inquiry_unresolved.count %></span>
        </div>
      </div>
    </div>

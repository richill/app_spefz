<div class="user_dashboard_admin_transactions">
  <!-- upper_data_content -->
  <div class="medium-12 columns upper_data_content">
    <div class="medium-2 columns left_data responsive_data stats_tab_01">
      <div class="data_content">
        <div class="header"> total subscribed users</div>
        <div class="header_data">
          <span class="header_icon"><i class="fa fa-users" aria-hidden="true"></i></span>
          <span><%= @users.subcribed_users.count %></span>
          <div class="minor_info">£<%= (@users.subcribed_users.count) * (@premium_plan.price) %>/mth</div>
        </div>
      </div>
    </div>
    <div class="medium-2 columns middle_data responsive_data stats_tab_02">
      <div class="data_content">
        <div class="header"> subscribed users [male]</div>
        <div class="header_data"><span class="header_icon"><i class="fa fa-male" aria-hidden="true"></i></span>
          <span><%= @users.males.subcribed_users.count %></span>
          <div class="minor_info">£<%= (@users.males.subcribed_users.count) * (@premium_plan.price) %> [mth]</div>
        </div>
      </div>
    </div>
    <div class="medium-2 columns middle_data responsive_data stats_tab_03">
      <div class="data_content">
        <div class="header"> subscribed users [female]</div>
        <div class="header_data">
          <span class="header_icon"><i class="fa fa-female" aria-hidden="true"></i></span>
          <span><%= @users.females.subcribed_users.count %></span>
          <div class="minor_info">£<%= (@users.females.subcribed_users.count) * (@premium_plan.price) %> [mth]</div>
        </div>
      </div>
    </div>
    <div class="medium-2 columns middle_data responsive_data stats_tab_04">
      <div class="data_content">
        <div class="header"> booked active events</div>
        <div class="header_data">
          <span class="header_icon"><i class="fa fa-calendar-check-o" aria-hidden="true"></i></span>
          <span><%= (@events.live_events.open_events).count - (@events.live_events.open_events.unbooked_events).count %></span>
          <div class="minor_info">£<%= @events.live_events.open_events.booked_events_all_total_price %> [all]</div>
        </div>
      </div>
    </div>
    <div class="medium-2 columns middle_data responsive_data stats_tab_05">
      <div class="data_content">
        <div class="header"> booked active events [mth]</div>
        <div class="header_data"><span class="header_icon"><i class="fa fa-calendar-check-o" aria-hidden="true"></i></span>
          <span><%= (@events.held_this_month.live_events.open_events).count - (@events.held_this_month.live_events.open_events.unbooked_events).count %></span>
          <div class="minor_info">£<%= @events.live_events.open_events.held_this_month.booked_events_all_total_price %> [mth]</div>
        </div>
      </div>
    </div>
    <div class="medium-2 columns right_data responsive_data stats_tab_06">
      <div class="data_content">
        <div class="header"> money in [Sub & Events]</div>
        <div class="header_data">
          <span class="header_icon"><i class="fa fa-gbp" aria-hidden="true"></i></span>
          <span>£<%= ((@users.subcribed_users.count) * (@premium_plan.price)) + ((@events.total_active_paid_events_heldThisMonth_amt) + (@events.total_active_free_events_heldThisMonth_amt)) %></span>
          <div class="minor_info">total amt in mth</div>
        </div>
      </div>
    </div>
  </div>


  <div class="clear"></div>

  <!-- table_data_content -->
  <div class="medium-12 columns lower_data_content stats_tab_01_content">
    <div class="medium-12 columns lower_data_inner_content">
      <%= render 'users/shared/data_info_subscribed_users_statsusers' %>
    </div>
  </div>

  <div class="medium-12 columns lower_data_content stats_tab_02_content hide">
    <div class="medium-12 columns lower_data_inner_content">
      <%= render 'users/shared/data_info_subscribed_male_users_statsusers' %>
    </div>
  </div>

  <div class="medium-12 columns lower_data_content stats_tab_03_content hide">
    <div class="medium-12 columns lower_data_inner_content">
      <%= render 'users/shared/data_info_subscribed_female_users_statsusers' %>
    </div>
  </div>

  <div class="medium-12 columns lower_data_content stats_tab_04_content hide">
    <div class="medium-12 columns lower_data_inner_content">
      <%= render 'users/shared/data_info_transactions_bookedactiveevents' %>
    </div>
  </div>

  <div class="medium-12 columns lower_data_content stats_tab_05_content hide">
    <div class="medium-12 columns lower_data_inner_content">
      <%= render 'users/shared/data_info_transactions_bookedactiveevents_mth' %>
    </div>
  </div>

   <div class="medium-12 columns lower_data_content stats_tab_06_content hide">
    <div class="medium-12 columns lower_data_inner_content">
      <%= render 'users/shared/data_info_transactions_moneyin' %>
    </div>
  </div>
  <div class="clear"></div>
</div>


<div class= "medium-12 columns user_general_settings_section user_main_dashboard_page">

  <!-- left_column_section -->
  <div class="medium-2 columns left_column_section">
    <%= render 'shared/content_user_adminpanel_left_column' %>
  </div>
  <!-- left_column_section -->


  <!-- right_column_section -->
  <div class="medium-10 columns right_column_section">
    <div class="right_inner_section">
      <!-- content -->
      <div class="user_general_settings_inner_section user_dashboard_section_main_content">
        <h1>transactions</h1>
        <ul>
          <li>1.] total subscribed users [£amt] / [below #]</li>
          <li>2.] total subscribed users [male] [£amt] / [below #]</li>
          <li>3.] total subscribed users [female] [£amt] / [below #]</li>
          <li>4.] total booked events [£amt all] / [below #]</li>
          <li>5.] total booked events [£amt mth] / [below #]</li>
          <li>6.] total money in (current_month) [total subscribed users [£amt/mth] + total booked events [£amt mth]]</li>
        </ul>

        <h1>payments content</h1>
        <ul>
          <li>firstname</li>
          <li>lastname</li>
          <li>spefz_payment_ref</li>
          <li>product</li>
          <li>stripe_customer_id</li>
          <li>stripe_subscription_id</li>
          <li>stripe_payment_id</li>
        </ul>
      </div>
      <!-- content -->
    </div>
  </div>
  <!-- right_column_section -->
</div>


        <div class="header"> management group</div>
        <div class="header_data"><span class="header_icon"><i class="fa fa-users" aria-hidden="true"></i></span><span><%= @users.management_group.count %></span></div>
      </div>
    </div>
    <div class="medium-2 columns middle_data responsive_data stats_tab_08">

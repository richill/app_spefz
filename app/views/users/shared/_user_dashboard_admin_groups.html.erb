        <div class="header"> client</div>
        <div class="header_data">
          <span class="header_icon"><i class="fa fa-users" aria-hidden="true"></i></span><span><%= @users.clients.count %></span>
        </div>
      </div>
    </div>
  </div>

  <div class="medium-12 columns upper_data_content">

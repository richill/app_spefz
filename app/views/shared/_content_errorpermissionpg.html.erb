<div class="medium-12 columns errorpermission_page_section">
  <div class="error_inner_container">
    <div class="error_content_section">
      <h2 class="header">Access Restricted</h2>
      <div class="subtitle">Access restricted to authourised personnel only</div>
      <div class="error_content_inner_section">
        <h1>Oops! </h1>
        <div><i class="fa fa-ban"></i></div>
        <h2>You are not authorize to access this page</h2>
        <div class="sub_content">contact your adminstrator on permission access</div>
        <div class="error_btn">
          <% if signed_in? %>
            <div class="btn_first"><%= link_to 'view socials', socials_path %></div>
            <div class="btn_second"><%= link_to 'view speed-events', events_path %></div>
          <% else %>
            <div class="btn_first"><%= link_to 'sign-up', new_user_registration_path %></div>
            <div class="btn_second"><%= link_to 'sign-in', user_session_path %></div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
